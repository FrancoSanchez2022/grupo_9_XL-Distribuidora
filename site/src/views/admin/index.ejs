            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a href="/admin/users" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Ver Listado</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/admin/addUser" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Añadir Usuario</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/admin/modUser" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Modificar Usuario</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/admin/users" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Eliminar Usuario</p>
                </a>
              </li>
            </ul>
          </li>
          <li class="nav-item">
            <a href="#" class="nav-link">
              <i class="fa-solid fa-users"></i>
              <p>
                Productos
                <i class="right fas fa-angle-left"></i>
              </p>
            </a>
            <ul class="nav nav-treeview">
              <li class="nav-item">
                <a href="/admin/list" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Ver productos</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/admin/create" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Añadir producto</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/admin/edit" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Modificar producto</p>
                </a>
              </li>
              <li class="nav-item">
                <a href="/admin/list" class="nav-link">
                  <i class="far fa-circle nav-icon"></i>
                  <p>Eliminar Usuario</p>
                </a>
              </li>
