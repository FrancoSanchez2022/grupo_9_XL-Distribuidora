<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head',{title:'XL - Nuestros Productos'}) %>

    <body class="liProduct">
        <%- include('./partials/header') %>
            <%- include('./partials/breadcrumbs',{pÃ¡gina:'Nuestros Productos'}) %>
                <main>
                    <h1 class="TituloPrincipal">Conoce nuestros productos</h1>
                    <section>
                        <% for( let i=0; i < 30; i++ ) { %>
                            <article class="product-relacionado">
                                <div class="contenedorRelacionado">
                                    <img src="/img/productos/<%= productos[i].imagenes[0] %> " alt="">
                                </div>
                                <h4 class="subproductos">
                                    <%= productos[i].titulo %>
                                </h4>
                                <span class="subtitular">
                                    <%= productos[i].descripcion %>
                                </span>
                                <p class="subprecio">
                                    <%= productos[i].precio %>
                                </p>
                                <button class="subcarritos">Agregar al carrito</button>

                            </article>
                            <% } %>
                    </section>
                </main>


                <%- include('./partials/footer') %>
    </body>

</html>